{
  "name": "jw-attendant-scheduler",
  "type": "nextjs",
  "description": "JW Attendant Scheduler - Kingdom Hall Event Management System",
  "version": "2.0.0",
  "environments": {
    "staging": {
      "container": 134,
      "ip": "10.92.3.24",
      "port": 3001,
      "url": "https://jw-staging.cloudigan.net"
    },
    "production": {
      "container": 132,
      "ip": "10.92.3.22", 
      "port": 3001,
      "url": "https://jw-production.cloudigan.net"
    },
    "database": {
      "container": 131,
      "ip": "10.92.3.21",
      "port": 5432,
      "name": "jw_attendant_scheduler"
    }
  },
  "authentication": {
    "type": "auth-stub",
    "provider": "custom",
    "adminEmail": "admin@jwscheduler.local",
    "adminPassword": "admin123"
  },
  "features": {
    "adminModule": true,
    "userManagement": true,
    "eventManagement": true,
    "attendantManagement": true,
    "countTimes": false
  },
  "deployment": {
    "method": "git-based",
    "branch": "staging",
    "cicd": true,
    "autoRestart": true
  },
  "wmacs": {
    "enforcement": "enhanced",
    "rules": [
      "WMACS-INFRA-001",
      "WMACS-CICD-001", 
      "WMACS-MERGE-001",
      "WMACS-AUTH-001"
    ],
    "foundationFirst": true,
    "complexityValidation": true
  }
}
