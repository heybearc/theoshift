# Theocratic Shift Scheduler - Environment Configuration
# Copy to .env and update values for your environment

# Database Configuration (PostgreSQL)
DATABASE_URL="postgresql://username:password@localhost:5432/theoshift_scheduler"

# JWT Authentication
JWT_SECRET="use [openssl rand -hex 32] to generate a 32 bytes value"

# Email Configuration
EMAIL_ENCRYPTION_KEY="your-32-character-encryption-key"-here

# Application Settings
NODE_ENV=production
PORT=3001
